{
  "name": "mydiaryapp",
  "version": "1.0.0",
  "description": "MyDiary is an online journal where users can pen down their thoughts and feelings.",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "dev:app": "concurrently 'cd server && npm run dev' 'cd client && npm start'",
    "install:client": "cd client && npm i",
    "install:server": "cd server && npm i",
    "prestart": "cd client && serve -s build",
    "install:app": "npm run install:client && npm run install:server",
    "start": "cd server && npm start",
    "test": "cd server && npm test",
    "pretest": "cd client && npm test",
    "coverage": "cd client && npm run coverage && cd .. && cd server && npm run coverage",
    "build": "cd server && npm build",
    "prebuild": "cd client && npm build",
    "start-ui-dev": "cd client && npm start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/okezieobi/MyDiaryApp.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/okezieobi/MyDiaryApp/issues"
  },
  "homepage": "https://github.com/okezieobi/MyDiaryApp#readme",
  "devDependencies": {
    "concurrently": "latest"
  }
}
