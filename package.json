  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/okezieobi/MyDiaryApp/issues"
  },
  "homepage": "https://github.com/okezieobi/MyDiaryApp#readme",
  "devDependencies": {
    "@babel/cli": "",
    "@babel/core": "",
    "@babel/node": "",
    "@babel/preset-env": "",
    "@babel/register": "",
    "coveralls": "",
    "eslint": "^6.7.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "mocha": "",
    "mocha-lcov-reporter": "",
    "nodemon": "",
    "nyc": ""
  },
  "dependencies": {
    "bcryptjs": "",
    "bluebird": "",
    "body-parser": "",
    "chai": "",
    "chai-http": "",
    "core-js": "",
    "cors": "",
    "dotenv": "",
    "express": "",
    "jsonwebtoken": "",
    "moment": "^2.24.0",
    "pg": "",
    "pg-promise": "",
    "regenerator-runtime": "",
    "swagger-jsdoc": "",
    "swagger-ui-express": "",
    "underscore": "",
    "unique-random": ""
  },
  "heroku-run-build-script": true
