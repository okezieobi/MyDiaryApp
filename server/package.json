  "bugs": {
    "url": "https://github.com/okezieobi/MyDiaryApp/issues"
  },
  "homepage": "https://github.com/okezieobi/MyDiaryApp#readme",
  "devDependencies": {
    "@babel/cli": "latest",
    "@babel/core": "latest",
    "@babel/node": "latest",
    "@babel/preset-env": "latest",
    "@babel/register": "latest",
    "babel-jest": "latest",
    "babel-register": "latest",
    "coveralls": "latest",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "jest": "latest",
    "jest-chain": "latest",
    "jest-extended": "latest",
    "nodemon": "latest",
    "nyc": "latest",
    "sequelize-cli": "latest",
    "superagent": "latest",
    "supertest": "latest"
  },
  "dependencies": {
    "@hapi/joi": "latest",
    "bcryptjs": "latest",
